<div data-id="<%= bookmark.id %>" data-video="<%= bookmark.video.id %>" class="news-item custom-item bookmarks">
  <div class="row">
    <div class="bookmark news-content no-tip col-md-4">
      <% if logged_in? %>
        <%= link_to '✎', '#', remote: true, class: 'edit-bookmark-button', title: 'Edit Bookmark' %>
        <%= link_to '✗', '#', remote: true, class: 'delete-bookmark-button', title: 'Delete Bookmark' %>
      <% end %>
      <!-- <a href="#" class="bookmark" data-time="<%= bookmark.start_time %>"><%= bookmark.title %></a> -->
      <a data-bind="value: title" href="#" class="bookmark" data-time="<%= bookmark.start_time %>"></a>
    </div>
  </div>
</div>
