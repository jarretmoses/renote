<% if @user_videos_set && @user_videos_set.present? %>

<section id="portfolio" class="gray-bg padding-top-bottom">
      <div class="container">
        <h3>My Notes</h3>
        
            <div class="projects-container scrollimation in">
                  <% @user_videos_set.each do |video_set| %>
                    <div class="row">
                      <% video_set.each do |video| %>
                        <%= render video %>
                      <% end %>
                    </div><!-- End Row -->
                  <% end %>
            </div><!-- End Projects Container -->        
      </div><!-- End container -->
      
  </section>

<% else %>
  <section id="portfolio" class="gray-bg padding-top-bottom">
      <div class="container">
        <h3>You don't have any notes on videos so far!</h3>
      </div>
  </section>
<% end %>